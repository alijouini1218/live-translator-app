{
  "version": 2,
  "buildCommand": "pnpm build:web",
  "installCommand": "pnpm install --frozen-lockfile",
  "outputDirectory": "apps/web/.next",
  "framework": null,
  "nodeVersion": "18.x",
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ],
  "functions": {
    "apps/web/src/app/api/*/route.ts": {
      "maxDuration": 30
    }
  }
}